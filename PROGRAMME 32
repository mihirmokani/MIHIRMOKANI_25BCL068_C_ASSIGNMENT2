#include <stdio.h>
int main() {
    int count = 0, flag;
    for (int i = 2; i <= 500; i++) {
        flag = 1;
        for (int j = 2; j * j <= i; j++) {
            if (i % j == 0) {
                flag = 0;
                break;
            }
        }
        if (flag)
            count++;
    }
    printf("Total prime numbers between 1 and 500 = %d", count);
    return 0;
}
