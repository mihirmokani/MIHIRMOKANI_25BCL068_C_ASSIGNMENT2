#include <stdio.h>
int main() {
    int num, sum=0, temp, rem;
    printf("Enter a number: ");
    scanf("%d", &num);
    temp = num;
    while(temp>0){
        rem = temp%10;
        sum += rem*rem*rem;
        temp/=10;
    }
    if(sum==num) printf("Armstrong");
    else printf("Not Armstrong");
    return 0;
}
