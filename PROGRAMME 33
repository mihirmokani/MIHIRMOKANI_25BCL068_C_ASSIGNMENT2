#include <stdio.h>
#include <math.h>
int main() {
    int num, sq, temp, digits = 0;
    printf("Enter a number: ");
    scanf("%d", &num);
    sq = num * num;
    temp = num;
    while (temp > 0) {
        digits++;
        temp /= 10;
    }
    int last = sq % (int)pow(10, digits);
    if (last == num)
        printf("%d is an Automorphic number.", num);
    else
        printf("%d is not an Automorphic number.", num);
    return 0;
}
